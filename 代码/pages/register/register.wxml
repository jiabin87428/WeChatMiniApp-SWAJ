<view class="baseView">
  <scroll-view scroll-y="true" class='scrollView'>
    <view class='scrollItem'>
      <text class='cellText1'>企业全称</text>
      <!-- <text class='cellText2'>{{companyName}}</text> -->
      <input id='companyName' class='input' placeholder="请输入企业全称" bindinput='textInput' placeholder-style='font-size:14px;font-weight: initial; color: #D0D0D0;' auto-focus/> 
    </view>
    <view id='companyPlace' class='scrollItem' bindtap='jumpRadio'>
      <text class='cellText1'>企业属地</text>
      <text class='cellText2'>{{companyPlace.name}}</text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view id='companyType1' class='scrollItem' bindtap='jumpRadio'>
      <text class='cellText1'>企业类型</text>
      <text class='cellText2'>{{companyType1.name}}/{{companyType2.name}}</text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view id='range' class='scrollItem' bindtap='jumpCheckBox'>
      <text class='cellText1'>涉及范围</text>
      <text class='cellText2'>{{rangeString}}</text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view class='scrollItem'>
      <text class='cellText1'>系统负责人</text>
      <!-- <text class='cellText2'>{{contactName}}</text> -->
       <input id='contactName' class='input' placeholder="请输入系统负责人" bindinput='textInput' placeholder-style='font-size:14px;font-weight: initial; color: #D0D0D0;' auto-focus/> 
    </view>
    <view class='scrollItem'>
      <text class='cellText1'>联系人邮箱</text>
      <!-- <text class='cellText2'>{{contactEmail}}</text> -->
       <input id='contactEmail' class='input' placeholder="将作为登录用户名" bindinput='textInput' placeholder-style='font-size:14px;font-weight: initial; color: #D0D0D0;' auto-focus/> 
    </view>
    <view class='scrollItem'>
      <text class='cellText1'>密码</text>
      <!-- <text class='cellText2'>{{password}}</text> -->
       <input id='password' class='input' placeholder="请输入企业全称" bindinput='textInput' placeholder-style='font-size:14px;font-weight: initial; color: #D0D0D0;' auto-focus password/> 
    </view>
    <view class='scrollItem'>
      <text class='cellText1'>确认密码</text>
      <!-- <text class='cellText2'>{{comformPassword}}</text> -->
       <input id='comformPassword' class='input' placeholder="请输入企业全称" bindinput='textInput' placeholder-style='font-size:14px;font-weight: initial; color: #D0D0D0;' auto-focus password/> 
    </view>
    <view class='scrollItem'>
      <text class='cellText1'>手机</text>
      <!-- <text class='cellText2'>{{phone}}</text> -->
       <input id='phone' class='input' placeholder="请输入企业全称" bindinput='textInput' placeholder-style='font-size:14px;font-weight: initial; color: #D0D0D0;' auto-focus/> 
    </view>
    <view id='address' class='scrollItem'>
      <text class='cellText1'>当前位置</text>
      <text class='cellText2'>{{address}}</text>
      <image class="arrow" mode="aspectFit"></image>
    </view>
    <view id='submit' class='scrollButtonItem'>
      <button class='submitButton' bindtap="submitClick">注册</button>
    </view>
  </scroll-view>
</view>
